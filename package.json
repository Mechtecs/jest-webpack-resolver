{
  "name": "jest-webpack-resolver",
  "version": "0.0.1",
  "description": "It is a jest plugin for enchanging jest resolving behaviour to webpack resolver through the webpack.config.js",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/jest",
    "prettier": "node_modules/.bin/prettier *.js --write"
  },
  "author": "mkg0",
  "license": "MIT",
  "devDependencies": {
    "directory-named-webpack-plugin": "^2.1.0",
    "jest": "^20.0.4",
    "prettier": "^1.4.4",
    "webpack": "^2.6.1"
  },
  "jest": {
    "resolver": "./index.js"
  },
  "jestWebpackResolver": {
    "webpackConfig": "./__mocks__/webpack.config.js"
  },
  "dependencies": {
    "enhanced-resolve": "^3.1.0",
    "pkg-dir": "^2.0.0"
  },
  "peerDependencies": {
    "jest": ">= 20.0.0"
  }
}
